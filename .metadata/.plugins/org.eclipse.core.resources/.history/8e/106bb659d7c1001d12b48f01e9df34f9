server.port = 8080
server.contextPath = /context-path

# JDBC PROPERTIES

spring.datasource.url=jdbc:mysql://localhost:3306/fitbuddy?useSSL=false&allowPublicKeyRetrieval=true

spring.datasource.username=root
spring.datasource.password=password

# HIBERNATE PROPERTIES

